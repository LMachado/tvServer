<h1>Novo Filme</h1>
<br>
<%= form_for :movie, url: movies_path, :html => { :multipart => true } do |f| %>
  <p>


    <%= f.label :Título %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :Duração %><br>
    <%= f.text_field :time %>
  </p>
  <p>
    <%= f.label :"Faixa etária"%><br>
    <%= f.text_field :age %>
  </p>
  <p>
    <%= f.label :"Localização da Foto" %><br>
    <%=f.file_field :foto%>
  </p>
  <p>
    <%= f.label :"Localização do Filme" %><br>
    <%=f.file_field :video%>
  </p>
   <p>
    <%= f.label :text %><br>
    <%= f.text_area :description %>
  </p>
  
  <p>
    <%= f.submit %>
  </p>
<% end %>